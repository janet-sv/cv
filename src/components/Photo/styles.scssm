@import "~sass/utils/index";

.photo {
  border: 1px solid $black;
  border-radius: 140px;
  height: 190px;
  width: 190px;
  margin: auto;
  padding: 8px;
}

.wrapper {
  overflow: hidden;
  display: inline-block;
  background-position: 50%;
  background-repeat: no-repeat;
  background-size: 100% auto;
  vertical-align: middle;
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.image {
  max-height: none;
}